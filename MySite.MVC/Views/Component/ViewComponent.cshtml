﻿@model List<MainMenu>


    @foreach (var menu in Model.OrderBy(p=>p.OrderNo))
    {
      
        <li class="nav-item">
            <a asp-action="@menu.ActionName"
            asp-controller="@menu.ControllerName"
            asp-area="@menu.AreaName" class="@menu.ClassName">
                <i class="@menu.IconName"></i>
                <p>@menu.MenuName</p>

            </a>

        </li>
    }